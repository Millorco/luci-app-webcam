#
# Webcam setting
#
config general 'general'
        option maintenance_mode 'no'
        option latitude '12'
        option longitude '14'
        option photo_name 'raw'
        option temp_scale 'c'
        option heating '1'

config shooting 'shooting'
        option imageformat '0'
        option imagesize '0'
        option imagequality '0'
        option whitebalance '0'

config day 'day'
        option iso_day '0'
        option aperture_day '0'
        option shutterspeed_day '0'

config night 'night'
        option iso_night '0'
        option aperture_night '0'
        option shutterspeed_night '0'

config server 'server'
        option upload_server 'example.com'
        option upload_directory 'photos'
        option upload_username 'user'
        option upload_password 'passw'

